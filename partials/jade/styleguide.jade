.box
    h1.underlined Tables

    table.table
        thead
            tr
                th Name
                th Passport Number
                th Activity
                th Actions
        tbody
            tr(ng-repeat="person in people")
                td {{person.name}}
                td {{person.passport}}
                td {{person.profession}}
                td
                    a.btn.btn-mini(ng-click="alert(person.id)") Delete

