.box
    //- BUTTONS
    h1.underlined Buttons

    h3 Normal size
    a.btn Simple .btn
    a.btn.btn-white .btn.btnwhite
    a.btn.btn-primary .btn.btn-primary
    a.btn.btn-secondary .btn.btn-secondary
    a.btn.btn-danger .btn.btn-danger
    a.btn.btn-invert .btn.btn-invert

    .spacer-3
    h3 Normal size + relevance
    a.btn.btn-hero Simple .btn
    a.btn.btn-white.btn-hero .btn.btnwhite
    a.btn.btn-primary.btn-hero .btn.btn-primary
    a.btn.btn-secondary.btn-hero .btn.btn-secondary
    a.btn.btn-invert.btn-hero .btn.btn-invert

    .spacer-3
    h3 Mini size
    a.btn.btn-mini .btn.btn-mini
    a.btn.btn-mini.btn-white .btn.btnwhite
    a.btn.btn-mini.btn-primary .btn.btn-primary
    a.btn.btn-mini.btn-secondary .btn.btn-secondary
    a.btn.btn-mini.btn-invert .btn.btn-invert

    .spacer-3
    h3 Disabled
    a.btn(disabled) .btn
    a.btn.btn-primary(disabled) .btn.btn-primary
    a.btn.btn-secondary(disabled) .btn.btn-secondary
    a.btn.btn-invert(disabled) .btn.btn-invert


